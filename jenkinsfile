pipeline {
    agent any
    stages {
     stage('---checkout---') {
            steps {
                sh "git clone https://github.com/cherukurisai451/maven_demo.git"
            }
        }
        stage('---clean---') {
            steps {
                sh "mvn clean"
            }
        }
       stage('--test--') {
            steps {
                sh "mvn test"
            }
        }
        stage('--package--') {
            steps {
                sh "mvn package"
            }
        }
    }
}
